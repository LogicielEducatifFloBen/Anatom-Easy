<?php

namespace FloBen\AnatomEasyBundle\Entity;

use FOS\UserBundle\Entity\User as BaseUser;
use Doctrine\ORM\Mapping as ORM;

/**
 * Userr
 * * @ORM\Table(name="User")
 */
class Userr extends BaseUser
{
    /**
     * @var integer
     */
    protected $id;

    /**
     * @var string
     */
    protected $name;

    /**
     * @var string
     */
    protected $firstName;
    /**
     * @var \FloBen\AnatomEasyBundle\Entity\Group
     */
    protected $groupe;

    /**
     * Set name
     *
     * @param string $name
     * @return Userr
     */
    public function setName($name)
    {
        $this->name = $name;
    
        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }

    /**
     * Set firstName
     *
     * @param string $firstName
     * @return Userr
     */
    public function setFirstName($firstName)
    {
        $this->firstName = $firstName;
    
        return $this;
    }

    /**
     * Get firstName
     *
     * @return string 
     */
    public function getFirstName()
    {
        return $this->firstName;
    }

    /**
     * Set groupe
     *
     * @param \FloBen\AnatomEasyBundle\Entity\Group $groupe
     * @return Userr
     */
    public function setGroupe(\FloBen\AnatomEasyBundle\Entity\Group $groupe = null)
    {
        $this->groupe = $groupe;
    
        return $this;
    }

    /**
     * Get groupe
     *
     * @return \FloBen\AnatomEasyBundle\Entity\Group 
     */
    public function getGroupe()
    {
        return $this->groupe;
    }
}
